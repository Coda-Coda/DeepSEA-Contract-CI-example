nix-build &> nix_build.log
nix-shell --run "
    rm -f FunctionalCorrectness.vo
    rm -f FunctionalCorrectness.glob
    compile-for-coq &> compile_contract.log
    compile-FunctionalCorrectness &> compile_FunctionalCorrectness.log
    if test -f \"./FunctionalCorrectness.vo\"; then
        echo \" ✅ Test Passed\"
    else
        echo \" ❌ Test FAILED\"
    fi
    "